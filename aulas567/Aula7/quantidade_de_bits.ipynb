    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "\n",
    "palavra = \"ABACATEIRO\"\n",
    "probs = np.array(  [palavra.count(w) for w in set(palavra)])/len(palavra)\n",
    "E = stats.entropy(probs, base=2)\n",
    "np.ceil(E*len(palavra))"
   ]
  }
